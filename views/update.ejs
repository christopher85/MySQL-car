<%- include("./partials/header") %> 

<div class="container">
    <div class="row">
        <div class="col-md-12">

            <form>
                <div class="form-group">
                  <label for="exampleFormControlInput1">Modèle</label>
                  <input type="text" class="form-control" value="<%= cars.modèle %> " >
                </div>
                <div class="form-group">
                  <label for="exampleFormControlSelect1">Constructeur</label>
                  <select class="form-control" >

                    <% factories.forEach(function(factory) {%>

                    <option value="<%= factory.id %>"> <%= factory.name %></option>
                
                    <%});%>

                  </select>
                </div>
                <div class="form-group">
                  <label for="exampleFormControlSelect2">carburant</label>
                  <select  class="form-control" >
                    <% energies.forEach(function(energy) {%>

                    <option value="<%= energy.id %>"> <%= energy.name %> </option>
                    
                    <%});%>

                  </select>
                </div>
                <div class="form-group">
                  <label for="exampleFormControlTextarea1">Example textarea</label>
                  <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
                </div>
              </form>

        </div>
    </div>
</div>



<%- include("./partials/footer") %>